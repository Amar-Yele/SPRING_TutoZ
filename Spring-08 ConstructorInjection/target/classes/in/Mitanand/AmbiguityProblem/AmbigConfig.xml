<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Define beans here -->
    
    
    <bean id="addInt" class="in.Mitanand.AmbiguityProblem.Addtion">
		<constructor-arg value="20" type="int"/>
		<constructor-arg value="40" type="int"/>
    </bean>
    <bean id="addString" class="in.Mitanand.AmbiguityProblem.Addtion">
		<constructor-arg value="400" type="String" index="1"/>
		<constructor-arg value="100" type="String" index="0"/>
    </bean>
    <bean id="addDouble" class="in.Mitanand.AmbiguityProblem.Addtion">
		<constructor-arg value="55.50" type="double" index="1"/>
		<constructor-arg value="40.08" type="double" index="0"/>
    </bean>
    
</beans>